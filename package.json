    "typescript": "^5.3.2",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.6",
    "wagtail-js": "1.1.1"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "prettier": "latest"
